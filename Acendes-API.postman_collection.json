        {
          "name": "3.3 Actualizar Partner",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"jsonrpc\": \"2.0\",\n  \"method\": \"call\",\n  \"params\": {\n    \"service\": \"object\",\n    \"method\": \"execute_kw\",\n    \"args\": [\n      \"{{db}}\",\n      {{uid}},\n      \"{{password}}\",\n      \"res.partner\",\n      \"write\",\n      [[{{partner_id}}], {\n        \"mobile\": \"3339999999\",\n        \"laboral_empresa\": \"Nueva Empresa SA\",\n        \"laboral_salario_mensual\": 40000.00,\n        \"email\": \"nuevo.email@test.com\"\n      }]\n    ]\n  },\n  \"id\": 32\n}"
            },
            "url": "{{base_url}}/jsonrpc",
            "description": "Actualiza campos del partner. Solo enviar los campos que cambian. Retorna true si éxito."
